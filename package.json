    "power-assert": "^1.4.4",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.2",
    "ts-node": "^4.0.2",
    "tslint": "^5.6.0",
    "typescript": "^2.6.1"
