    "toml": "^2.3.3"
  },
  "devDependencies": {
    "@nestjs/common": "^4.5.1",
    "@nestjs/core": "^4.5.1",
    "@nestjs/microservices": "^4.5.1",
    "@nestjs/testing": "^4.5.1",
    "@nestjs/websockets": "^4.5.1",
    "@types/bignumber.js": "^4.0.3",
    "@types/config": "0.0.33",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.5.1",
